{
  "name": "nasm-x86-assembly-language",
  "displayName": "NASM X86 Assembly Language",
  "description": "Support NASM x86 assembly language",
  "version": "0.0.2",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.78.0"
  },
  "author": "ykkz000",
  "publisher": "ykkz000",
  "license": "The MIT LICENSE",
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extensions.js",
  "activationEvents": [],
  "contributes": {
    "languages": [
      {
        "id": "assembly",
        "aliases": [
          "NASM X86 Assembly",
          "assembly",
          "asm"
        ],
        "extensions": [
          ".asm",
          ".nasm",
          ".s",
          ".inc"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "assembly",
        "scopeName": "source.asm",
        "path": "./syntaxes/assembly.tmLanguage.json"
      }
    ]
  },
  "repository": {
    "url": "https://github.com/ykkz000/nasm-x86-assembly-language",
    "type": "git"
  }
}